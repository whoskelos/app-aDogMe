				<script src="/js/bootstrap.min.js"></script>
				<script src="https://kit.fontawesome.com/78a201790e.js" crossorigin="anonymous"></script>
				{{ encore_entry_script_tags('app') }}
			{% endblock %}
		</body>
	</html>
